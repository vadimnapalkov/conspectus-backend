    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "4.0.1",
    "flat": "^4.1.0",
    "husky": "3.0.4 ",
    "lint-staged": "9.2.4",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "repl": "^0.1.3",
    "typescript": "3.5.3"
  },
  "engines": {
    "node": ">= 10"
  }
